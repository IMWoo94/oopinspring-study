package expert003;

public class Car {
	Tire tire;
	Tire rTirea;

	public Tire getTire() {
		return tire;
	}

	public void setTire(Tire tire) {
		this.tire = tire;
	}
	
	public void setRTire(Tire tire) {
		this.rTirea = tire;
	}

	public String getTireBrand() {
		return "장착된 타이어: " + tire.getBrand() + "오른 " + rTirea.getBrand();
	}
}