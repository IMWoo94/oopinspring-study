package expert003;

public class Car {
	Tire tire;
	Tire rTire;

	public Tire getTire() {
		return tire;
	}

	public void setTire(Tire tire) {
		this.tire = tire;
	}
	
	public void setRTire(Tire rTire) {
		this.rTire = rTire;
	}

	public String getTireBrand() {
		return "장착된 타이어: " + tire.getBrand() + "오른 " + rTire.getBrand();
	}
}